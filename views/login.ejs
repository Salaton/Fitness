<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" >
<link rel="stylesheet" href="CSS/style.css">


<div class = "limiter">
  <div class="container-login100">
    <div class="wrap-login100">

<form action="/login" method="POST" class="login_form" >
  <h1 class = "page-heading">Login to continue</h1>

      <div class="content">
        <div class="form-group">
          <label for="Username"></label>
          <input class="form-control" name="username" type="text"  required placeholder="Username">
        </div>
        <div class="form-group">
          <label for="password"></label>
          <input class="form-control" name="password" type="password"  placeholder="Password" required  pattern=".{4,10}" required title="4 to 10 characters">
          
        </div>
        <div>
          <label for="Remember">Remember me?</label>
          <input class="Remember" id="Remember" type="checkbox" name="Remember" checked >
        </div>
        <button class="btn btn-primary btn-block">Login</button>
        <div class="text-center">
          <span class="txt1">Don't have an account?</span>
          <a class="txt2" href="signup" >Sign Up</a>
        </div>
        <div class="text-center">
          <span class="txt1">Not a member?</span>
          <a class="txt2" href="adminlogin" >Sign in as Admin</a>
        </div>
      </div>
    

</form>
<div class="login100-more" style="background-image: url('/img-gif/graphic3.svg');">
				</div>
</div>
</div>
</div>

  
